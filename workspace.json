{
  "version": 2,
  "projects": {
    "better-stringify": {
      "root": "packages/better-stringify",
      "sourceRoot": "packages/better-stringify/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/better-stringify",
            "main": "packages/better-stringify/src/index.ts",
            "tsConfig": "packages/better-stringify/tsconfig.lib.json",
            "assets": ["packages/better-stringify/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/better-stringify/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/better-stringify"],
          "options": {
            "jestConfig": "packages/better-stringify/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "memoize": {
      "root": "packages/memoize",
      "sourceRoot": "packages/memoize/src",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "project": "packages/memoize/package.json",

            "outputPath": "dist/packages/memoize",
            "entryFile": "packages/memoize/src/index.ts",
            "tsConfig": "packages/memoize/tsconfig.lib.json",
            "assets": ["packages/memoize/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/memoize/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/memoize"],
          "options": {
            "jestConfig": "packages/memoize/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "simple": {
      "root": "packages\\simple",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/simple",
            "tsConfig": "packages/simple/tsconfig.lib.json",
            "packageJson": "packages/simple/package.json",
            "main": "packages/simple/index.ts",
            "assets": ["packages/simple/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
